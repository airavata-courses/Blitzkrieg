node {
    stage('Code checkout') {
        checkout scm
    }

    stage('Build image') {
	    	bat "docker-compose -f .\\user_management\\docker-compose.yml build"
    }
	
    stage('Push image') {
        docker.withRegistry('https://registry.hub.docker.com', 'docker-hub-credentials') {
	    bat "docker login -u thertion12 -p *Batu_20110656*"
            bat "docker push thertion12/user_management"
        }
    }
}
